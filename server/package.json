    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@common/(.*)$": "<rootDir>/../src/common/$1",
      "^@test/(.*)$": "<rootDir>/../test/$1"
